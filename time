		obj:Destroy()
	end
end
wait(0.01)
player.Character:PivotTo(CFrame.new(122.102119, 25.2653675, 293.168396, -0.998823166, -4.63327865e-08, -0.0485009402, -4.19356745e-08, 1, -9.16779328e-08, 0.0485009402, -8.95361225e-08, -0.998823166))
